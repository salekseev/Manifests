# /etc/dhcp/dhcpd.conf for <%= datacenter %>
# Hostname: <%= fqdn %>
#
# WARNING: This file is managed by PUPPET, any changes made will be lost!
#
authoritative;
ddns-update-style none;
#ignore client-updates;
allow booting;
allow bootp;
log-facility local6;


option option-176 code 176 = string;
option option-242 code 242 = string;

### Omapi key for remote API access
key omapi_key {
    algorithm HMAC-MD5;
        secret "L0xUoWjkMoCYHv6FIFjXvMpBTCqM7PAaydlPlnwAGL5PlXwTvkrnp/qYsmfIQD1PVFy3KckjdBdgPBpyOX75Kw==";
};
omapi-port 7911;
omapi-key omapi_key;

group st2-cobbler {
    filename "/pxelinux.0";
    next-server 10.125.140.25;
}

